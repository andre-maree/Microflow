{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "logReplayEvents": "false",
      "hubName": "MicroflowTaskHub"
    },
    "http": {
      "routePrefix": ""
    }
  },
  "logging": {
    "logLevel": {
      "DurableTask.AzureStorage": "None",
      "DurableTask.Core": "None"
    },
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": false
      }
    }
  }
}